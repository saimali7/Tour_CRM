{
  "name": "@tour/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./src/schema/index.ts"
    }
  },
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx scripts/apply-migration.ts",
    "db:seed": "tsx src/seed/index.ts",
    "db:add-member": "tsx src/seed/add-member.ts",
    "studio": "drizzle-kit studio",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@supabase/supabase-js": "^2.47.10",
    "drizzle-orm": "^0.38.2",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "@tour/eslint-config": "workspace:*",
    "@tour/typescript-config": "workspace:*",
    "@types/node": "^22.10.2",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.57.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
